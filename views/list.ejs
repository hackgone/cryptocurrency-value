<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
  <link rel="stylesheet" href="css/style.css">
  </head>
  <body style="background-color:#191d28">

    <nav>
      <img class="im" src="logo.png" >
      <ul>
            <li class="cover">INR</li>
            <li class="cover">BTC</li>

      </ul>
    </nav>

    <table class="new">
      <tr class="cover">

          <th>#</th>
          <th>Name</th>
          <th>last</th>
          <th>Buy/Sell Price</th>
          <th>Volume</th>
          <th>base_unit</th>


      </tr>
        <%for (var i=0;i<10;i++){%>
          <tr class="cover">
          <th><%=i%></th>
          <th><%=all[i][1].name%></th>
          <th><%=all[i][1].last%></th>
          <th><%=all[i][1].buy%></th>
          <th><%=all[i][1].volume%></th>
          <th><%=all[i][1].base_unit%></th>
          </tr>
        <%}%>


    </table>

  </body>
</html>
